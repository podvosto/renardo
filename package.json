{
  "name": "renardo",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc --incremental",
    "build:watch": "tsc --incremental --watch",
    "tsc": "tsc --incremental",
    "start": "node build/app.js",
    "start:dev": "node -r dotenv/config -r ts-node/register --inspect src/app.ts",
    "start:dev:watch": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec yarn start:dev",
    "start:playground": "node -r dotenv/config -r ts-node/register --inspect src/playground.ts",
    "start:playground:watch": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec yarn start:playground",
    "contract:compile": "truffle compile",
    "contract:dev:server": "ganache-cli -p 8545",
    "contract:dev:deploy": "truffle migrate --network=polygon_testnet",
    "contract:deploy": "truffle migrate --network=polygon_mainnet",
    "update-abis": "node scripts/update_abis.js",
    "deploy:renardo": "pm2 deploy pm2.json mainnet",
    "deploy:env": "scp .env.* deployer@renardo-server:/home/deployer"
  },
  "dependencies": {
    "@truffle/hdwallet-provider": "^1.5.0",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "bignumber.js": "^9.0.1",
    "colors": "^1.4.0",
    "dotenv": "^10.0.0",
    "ethereum-multicall": "^2.7.1",
    "ethers": "^5.4.4",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/node": "^16.6.1",
    "@types/node-fetch": "^2.5.12",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "ganache-cli": "^6.12.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "truffle": "^5.4.7",
    "ts-node": "^10.2.0",
    "tslib": "^2.3.1",
    "typescript": "^4.3.5"
  }
}
